<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="Groovy">
        <added pattern="com.madchampionship.Prototypes.team(name)" />
        <added pattern="com.madchampionship.Prototypes.player(name)" />
        <added pattern="com.google.appengine.api.taskqueue.TaskOptions.taskName(taskName)" />
        <added pattern="com.google.appengine.api.taskqueue.Queue.deleteTask(s)" />
        <added pattern="geb.Page.$(selector)" />
        <added pattern="org.codehaus.groovy.runtime.DefaultGroovyStaticMethods.sleep(self,milliseconds)" />
        <added pattern="org.brunocvcunha.instagram4j.Instagram4j.Instagram4jBuilder.username(username)" />
      </blacklist>
    </blacklists>
  </component>
</application>